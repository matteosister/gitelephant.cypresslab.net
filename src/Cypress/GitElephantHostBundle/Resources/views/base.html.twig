{% extends "OryzoneBoilerplateBundle::html5.html.twig" %}

{% block head_css %}
    {{ parent() }}
    {% stylesheets filter="?uglifycss" output="css/compiled/all.css"
        "@CypressGitElephantHostBundle/Resources/public/css/bootstrap/bootstrap.css"
        "@CypressGitElephantHostBundle/Resources/public/css/bootstrap/responsive.css"
        "@CypressGitElephantHostBundle/Resources/public/compass/stylesheets/screen.css"
        "@HighlightBundle/Resources/public/*.css" %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body_container_header %}
    {% render "CypressGitElephantHostBundle:Home:header" with { standalone: true } %}
{% endblock %}

{% block body_js_jquery %}{% endblock %}

{% block body_js %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
{% endblock %}