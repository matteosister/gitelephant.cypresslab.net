{% extends "OryzoneBoilerplateBundle::html5.html.twig" %}

{% block head_css %}
    {{ parent() }}
    {% stylesheets output="css/compiled/all.css"
        "@CypressGitElephantHostBundle/Resources/public/css/bootstrap/bootstrap.css"
        "@CypressGitElephantHostBundle/Resources/public/css/bootstrap/responsive.css" %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body_container_header %}
    {% render "CypressGitElephantHostBundle:Home:header" with { standalone: true } %}
{% endblock %}

{% block body_js %}
    {{ parent() }}
    {% javascripts output="js/compiled/all.js"
        "@CypressGitElephantHostBundle/Resources/public/bootstrap/js/bootstrap-dropdown.js" %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}