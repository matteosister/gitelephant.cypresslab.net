{% extends "CypressGitElephantHostBundle::base.html.twig" %}

{% block body_container_main %}
    <div class="row-fluid">
        {% include "CypressGitElephantHostBundle:Repository:tabs.html.twig" with { selected: 'commits' } %}
    </div>
    <ul>
    {% for commit in commits %}
        <li>{{ commit.message }} ({{ commit.sha }})</li>
    {% endfor %}
    </ul>
{% endblock %}