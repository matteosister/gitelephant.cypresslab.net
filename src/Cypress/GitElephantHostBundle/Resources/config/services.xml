<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="cypress.git_elephant_host.repository.listener" class="Cypress\GitElephantHostBundle\Listener\RepositoryBinderListener">
            <tag name="doctrine_mongodb.odm.event_listener" event="postLoad" />
        </service>

        <service id="cypress.git_elephant_host.twig_extension" class="Cypress\GitElephantHostBundle\Twig\CypressGitElephantHostExtension">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
