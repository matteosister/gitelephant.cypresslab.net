    <parameters>
        <parameter key="pygments_elephant.pygmentize_binary.class">PygmentsElephant\PygmentizeBinary</parameter>
        <parameter key="pygments_elephant.pygmentize.class">PygmentsElephant\Pygmentize</parameter>
    </parameters>

    <services>
        <service id="pygments_elephant.pygmentize_binary" class="%pygments_elephant.pygmentize_binary.class%"></service>

        <service id="pygments_elephant.pygmentize" class="%pygments_elephant.pygmentize.class%">
            <argument type="service" id="pygments_elephant.pygmentize_binary"></argument>
        </service>

        <service id="pygments_elephant.routing_loader" class="Cypress\PygmentsBundle\Routing\CssRoutingLoader">
